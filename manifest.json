{
  "manifest_version": 2,
  "author": "group_1",
  "applications": {
    "gecko": {
      "id": "helloworld@yoursite.com"
    }
  },
  "name": "PhishNet",
  "version": "1.0",
  "description": "AI phishing detection tool for email clients",
  "permissions": [
    "<all_urls>",
    "messagesRead",
